<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{site.title}} | {{page.title}}</title>
    <meta name="description" content="{{page.description}}">
    <meta name="keywords" content="{{page.keywords}}">
    
    <!-- Favicon -->
    <link rel="icon" type="image/png" sizes="32x32" href="{{site.favicon32}}">
    <link rel="icon" type="image/png" sizes="16x16" href="{{site.favicon16}}">
    <link rel="apple-touch-icon" sizes="180x180" href="{{site.appleTouchIcon}}">
    <link rel="manifest" href="{{site.webmanifest}}">
    
    <!-- Open Graph Meta Tags -->
    <meta property="og:title" content="{{site.title}} | {{page.title}}">
    <meta property="og:description" content="{{page.description}}">
    <meta property="og:type" content="website">
    <meta property="og:url" content="{{site.url}}">
    <meta property="og:image" content="{{site.ogImage}}">
    <meta property="og:site_name" content="{{site.brandName}}">
    
    <!-- Twitter Card Meta Tags -->
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:title" content="{{site.title}} | {{page.title}}">
    <meta name="twitter:description" content="{{page.description}}">
    <meta name="twitter:image" content="{{site.ogImage}}">
    
    <!-- Canonical URL -->
    <link rel="canonical" href="{{site.url}}{{page.canonical}}">
    
    <!-- Stylesheets -->
    <link rel="stylesheet" href="styles.css">
    {{#if page.customCSS}}
    <link rel="stylesheet" href="{{page.customCSS}}">
    {{/if}}
    
    <!-- Structured Data -->
    <script type="application/ld+json">
    {
        "@context": "https://schema.org",
        "@type": "Organization",
        "name": "{{site.brandName}}",
        "url": "{{site.url}}",
        {{#if site.logo}}
        "logo": "{{site.logo}}",
        {{/if}}
        {{#if contact}}
        "contactPoint": {
            "@type": "ContactPoint",
            "telephone": "{{contact.phone}}",
            "contactType": "customer service"
        },
        {{/if}}
        "sameAs": [
            {{#each site.socialMedia}}
            "{{this.url}}"{{#unless @last}},{{/unless}}
            {{/each}}
        ]
    }
    </script>
</head>
<body>
    <!-- Skip Navigation Link -->
    <a href="#main-content" class="skip-nav">Skip to main content</a>
    
    <!-- Navigation -->
    <nav class="nav" id="navigation" role="navigation" aria-label="Main navigation">
        <div class="nav-container">
            <div class="nav-content">
                <!-- Logo/Brand -->
                <a href="{{site.homeUrl}}" class="nav-logo" aria-label="{{site.brandName}} homepage">
                    {{#if site.logo}}
                    <img src="{{site.logo}}" alt="{{site.brandName}}" class="nav-logo-img">
                    {{else}}
                    {{site.brandName}}
                    {{/if}}
                </a>
                
                <!-- Desktop Navigation -->
                <div class="nav-links" role="menubar">
                    {{#each navigation.links}}
                    <a href="{{this.href}}" class="nav-link" role="menuitem"
                       {{#if this.external}}target="_blank" rel="noopener"{{/if}}>
                        {{this.text}}
                    </a>
                    {{/each}}
                </div>
                
                <!-- Navigation Buttons -->
                <div class="nav-buttons">
                    {{#if navigation.callButton}}
                    <a href="tel:{{navigation.callButton.phone}}" class="btn btn-ghost" aria-label="Call {{navigation.callButton.phone}}">
                        <svg class="icon" viewBox="0 0 24 24" fill="none" stroke="currentColor" aria-hidden="true">
                            <path d="M22 16.92v3a2 2 0 0 1-2.18 2 19.79 19.79 0 0 1-8.63-3.07 19.5 19.5 0 0 1-6-6 19.79 19.79 0 0 1-3.07-8.67A2 2 0 0 1 4.11 2h3a2 2 0 0 1 2 1.72 12.84 12.84 0 0 0 .7 2.81 2 2 0 0 1-.45 2.11L8.09 9.91a16 16 0 0 0 6 6l1.27-1.27a2 2 0 0 1 2.11-.45 12.84 12.84 0 0 0 2.81.7A2 2 0 0 1 22 16.92z"></path>
                        </svg>
                        <span>{{navigation.callButton.text}}</span>
                    </a>
                    {{/if}}
                    
                    {{#if navigation.quoteButton}}
                    <a href="{{navigation.quoteButton.href}}" class="btn btn-primary">
                        {{navigation.quoteButton.text}}
                    </a>
                    {{/if}}
                </div>
                
                <!-- Mobile Menu Button -->
                <button class="mobile-menu-btn" id="mobileMenuBtn" aria-label="Toggle menu" aria-expanded="false" aria-controls="mobileMenu">
                    <svg class="icon menu-icon" viewBox="0 0 24 24" fill="none" stroke="currentColor" aria-hidden="true">
                        <line x1="3" y1="12" x2="21" y2="12"></line>
                        <line x1="3" y1="6" x2="21" y2="6"></line>
                        <line x1="3" y1="18" x2="21" y2="18"></line>
                    </svg>
                    <svg class="icon close-icon hidden" viewBox="0 0 24 24" fill="none" stroke="currentColor" aria-hidden="true">
                        <line x1="18" y1="6" x2="6" y2="18"></line>
                        <line x1="6" y1="6" x2="18" y2="18"></line>
                    </svg>
                </button>
            </div>
            
            <!-- Mobile Menu -->
            <div class="mobile-menu hidden" id="mobileMenu" role="menu" aria-labelledby="mobileMenuBtn">
                <div class="mobile-menu-content">
                    {{#each navigation.links}}
                    <a href="{{this.href}}" class="mobile-menu-link" role="menuitem"
                       {{#if this.external}}target="_blank" rel="noopener"{{/if}}>
                        {{this.text}}
                    </a>
                    {{/each}}
                    
                    <div class="mobile-menu-buttons">
                        {{#if navigation.callButton}}
                        <a href="tel:{{navigation.callButton.phone}}" class="btn btn-outline">
                            <svg class="icon" viewBox="0 0 24 24" fill="none" stroke="currentColor" aria-hidden="true">
                                <path d="M22 16.92v3a2 2 0 0 1-2.18 2 19.79 19.79 0 0 1-8.63-3.07 19.5 19.5 0 0 1-6-6 19.79 19.79 0 0 1-3.07-8.67A2 2 0 0 1 4.11 2h3a2 2 0 0 1 2 1.72 12.84 12.84 0 0 0 .7 2.81 2 2 0 0 1-.45 2.11L8.09 9.91a16 16 0 0 0 6 6l1.27-1.27a2 2 0 0 1 2.11-.45 12.84 12.84 0 0 0 2.81.7A2 2 0 0 1 22 16.92z"></path>
                            </svg>
                            <span>{{navigation.callButton.text}}</span>
                        </a>
                        {{/if}}
                        
                        {{#if navigation.quoteButton}}
                        <a href="{{navigation.quoteButton.href}}" class="btn btn-primary">
                            {{navigation.quoteButton.text}}
                        </a>
                        {{/if}}
                    </div>
                </div>
            </div>
        </div>
    </nav>
    
    <!-- Main Content Wrapper -->
    <main id="main-content" role="main"> 